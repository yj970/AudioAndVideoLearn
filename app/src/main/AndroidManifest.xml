<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="cn.yj.audioandvideolearn">



    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.RECORD_AUDIO"/>


    <application
        android:name=".APP"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".learn1.ViewsActivity"/>
        <activity android:name=".learn1.ImageViewActivity"/>
        <activity android:name=".learn1.SurfaceViewActivity"/>
        <activity android:name=".learn1.CustomerViewActivity"/>
        <activity android:name=".learn2.AudioRecorderActivity"/>
        <activity android:name=".learn3.AudioTrackActivity"/>

    </application>

</manifest>